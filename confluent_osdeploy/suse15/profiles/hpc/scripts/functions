run_remote() {
    cd $(mktemp -d)
    curl -f https://$mgr/confluent-public/os/$profile/scripts/$1 > $1
    chmod +x $1
    cmd=$1
    shift
    ./$cmd $*
    cd -
}

run_remote_python() {
    cd $(mktemp -d)
    curl -f https://$mgr/confluent-public/os/$profile/scripts/$1 > $1
    python3 $*
    cd -
}
